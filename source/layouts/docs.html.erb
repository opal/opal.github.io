<% wrap_layout :layout do %>
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <h1><%= data.page.title %></h1>
      </div>
    </div>
    <div class="row">
      <div class="col-md-4 col-md-push-8">
        <%= table_of_contents current_page %>
      </div>

      <div class="col-md-8 col-md-pull-4">
        <%= yield %>
      </div>
    </div>
  </div>
<% end %>
